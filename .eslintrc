{
  "parser": "babel-eslint",
  "extends": ["plugin:prettier/recommended", "prettier", "prettier/react"],
  "plugins": ["prettier"],
  "env": {
    "commonjs": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "jsx": true,
      "ecmaVersion": 8
    },
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": "warn",
    "no-console": 0
  }
}
